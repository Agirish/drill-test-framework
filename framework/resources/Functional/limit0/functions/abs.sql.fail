select 
        abs(sum(o.c_integer)),
        abs(sum(r.c_integer)),
        abs(sum(o.c_bigint)),
        abs(sum(r.c_bigint)), 
        abs(sum(o.c_float)),
        abs(sum(r.c_float)), 
        abs(sum(o.c_double)),
        abs(sum(r.c_double))
from 
        optional_type_v as o,
        required_type_v as r
where
        o.c_date = r.c_date;
